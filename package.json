  "author": "LitenApe",
  "license": "MIT",
  "description": "React Component Library for personal use",
  "dependencies": {
    "@storybook/addon-actions": "^5.2.3",
    "@storybook/addon-info": "^5.2.3",
    "@storybook/addon-knobs": "^5.2.3",
    "@storybook/addon-links": "^5.2.3",
    "@storybook/addons": "^5.2.3",
    "@storybook/react": "^5.2.3",
    "cookednames": "^1.1.3",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-scripts": "3.1.1",
    "typescript": "3.6.2"
  },
  "scripts": {
    "start": "npm run start:storybook",
    "start:storybook": "start-storybook -p 6006",
    "build": "react-scripts build",
    "build:storybook": "build-storybook",
    "test": "react-scripts test --watchAll=false",
    "test:update": "react-scripts test --watchAll=false --updateSnapshot",
    "eject": "react-scripts eject",
    "lint": "eslint './src/**/*.{ts,tsx}' './stories/**/*.{ts,tsx}'",
    "lint:fix": "eslint --fix './src/**/*.{ts,tsx}' './stories/**/*.{ts,tsx}'"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm run test"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@storybook/addon-a11y": "^5.2.3",
    "@types/enzyme": "^3.10.3",
    "@types/jest": "24.0.18",
    "@types/node": "12.7.4",
    "@types/react": "^16.9.5",
    "@types/react-dom": "16.9.0",
    "@types/react-test-renderer": "^16.9.0",
    "@types/storybook__addon-info": "^4.1.2",
    "@types/storybook__react": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^2.3.3",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-typescript": "^4.0.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "husky": "^3.0.8",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "react-test-renderer": "^16.10.2"
  }
